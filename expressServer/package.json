{
	"name": "expressserver",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node-dev --respawn --transpile-only src/server.ts",
		"build": "tsc",
		"start": "node dist/server.js",
		"prepare": "husky",
		"lint:fix": "eslint --fix 'src/**/*.ts'",
		"precommit-check": "npm run type-check && eslint src --ext .ts && prettier --check .",
		"type-check": "tsc --noEmit",
		"lint": "eslint src --ext .ts",
		"format": "prettier --write ."
	},
	"lint-staged": {
		"src/**/*.ts": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"dependencies": {
		"body-parser": "^2.2.1",
		"dayjs": "^1.11.19",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"mqtt": "^5.14.1",
		"winston": "^3.18.3"
	},
	"devDependencies": {
		"@types/express": "^5.0.5",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/mqtt": "^0.0.34",
		"@types/node": "^24.10.1",
		"@types/winston": "^2.4.4",
		"@typescript-eslint/eslint-plugin": "^8.48.0",
		"@typescript-eslint/parser": "^8.48.0",
		"eslint": "^9.39.1",
		"eslint-plugin-prettier": "^5.5.4",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.9.3"
	}
}
